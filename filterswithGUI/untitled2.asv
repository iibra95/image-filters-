% --- Executes on selection change in popupmenu1.
function popupmenu1_Callback(hObject, eventdata, handles)
% hObject    handle to popupmenu1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
gambar = handles.data1;
idmode = get(hObject,'value');
if idmode==1
elseif idmode==2
    axes(handles.axes2);
    imshow(gambar);
elseif idmode==3
    axes(handles.axes2);
    A=rgb2gray(gambar);
    imshow(A);
elseif idmode==4
    axes(handles.axes2);
    A = rgb2gray(gambar);
    B = im2bw(A);
    imshow(B);
elseif idmode==5
    axes(handles.axes2);
    A = rgb2gray(gambar);
    imhist(A);
elseif idmode==6
    axes(handles.axes2);
    A=rgb2gray(gambar);
    B = histeq(A);
    imshow(B);
elseif idmode==7
    axes(handles.axes2);
    A=rgb2gray(gambar);
    open = strel('disk',5);
    set = imopen(A,open);
    imshow(set);
elseif idmode==8
    axes(handles.axes2);
    A = rgb2gray(gambar);
    B = strel('disk',10);
    C = imclose(A,B);
    imshow(C);
elseif idmode==9
    axes(handles.axes2);
    A = rgb2gray(gambar);
    B = strel('disk',11);
    C = imerode(A,B);
    imshow(C);
end

